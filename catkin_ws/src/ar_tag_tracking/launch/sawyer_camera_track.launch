<launch>
  <!-- AR Track Alvar Node for Sawyer wrist camera -->
  <node name="ar_track_alvar_sawyer" pkg="ar_track_alvar" type="individualMarkersNoKinect" output="screen"
        args="13 0.08 0.2 /io/internal_camera/right_hand_camera/image_raw /io/internal_camera/right_hand_camera/camera_info /right_hand_camera">
    <remap from="/ar_pose_marker" to="/sawyer/ar_pose_marker" />
  </node>
</launch>
